<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Mailing_Address_ZipCode</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
    NOT(ISNEW()),
    NOT(ISBLANK(MailingPostalCode )),
    LEN(MailingPostalCode ) &gt; 10
)</errorConditionFormula>
    <errorDisplayField>MailingPostalCode</errorDisplayField>
    <errorMessage>Character length should be less than 10.</errorMessage>
</ValidationRule>
