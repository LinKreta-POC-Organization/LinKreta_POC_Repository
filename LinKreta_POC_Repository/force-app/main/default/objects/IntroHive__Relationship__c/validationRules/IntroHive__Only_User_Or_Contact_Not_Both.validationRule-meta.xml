<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>IntroHive__Only_User_Or_Contact_Not_Both</fullName>
    <active>true</active>
    <errorConditionFormula>NOT (ISBLANK(IntroHive__User_Contact_From__c)  ||
